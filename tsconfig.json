{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2020",
		"outDir": "out",
		"lib": ["ES2020", "WebWorker"],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noImplicitAny": true,
		"noImplicitOverride": true,
		"noImplicitThis": true,
		"noUnusedParameters": true
	}
}
